<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <!-- primary|extension Inherited .:Form:. view for "model" -->
    <record id="view_asset_car_form_inherited" model="ir.ui.view">
        <field name="name">view.asset_car.form_inherited</field>
        <field name="model">account.asset</field>
        <field name="inherit_id" ref="account_asset.view_account_asset_form" />
        <field name="type">form</field>
        <!-- <field name="mode"></field> -->
        <field name="arch" type="xml">
          
          <xpath expr="//field[@name='name']" position="after">
            <group>
               <br/>
                <group>
                  <!-- <label for="car_is" string="Car Is"/>  -->
                  <field name="car_is"/>
                </group>
                <group>
                  <h6> <label for="vehicle_registration_expired" string="Vehicle Registration Expired" attrs="{'invisible':[('car_is','=',False)]}" /> 
                  <field name="vehicle_registration_expired"  attrs="{'invisible':[('car_is','=',False)]}" /> </h6>

                  <h6><label for="date_of_the_periodic_examination" string="Date Of The Periodic Examination" attrs="{'invisible':[('car_is','=',False)]}" /> 
                    <field name="date_of_the_periodic_examination" attrs="{'invisible':[('car_is','=',False)]}"/> </h6> 

                  <h6><label for="periodic_maintenance_history" string="Periodic Maintenance History" attrs="{'invisible':[('car_is','=',False)]}" /> 
                    <field name="periodic_maintenance_history" attrs="{'invisible':[('car_is','=',False)]}"/> </h6>   
                </group>
            </group>
          </xpath>
          
           
        </field>
    </record>

      <record id="view_dev_config_form_asset" model="ir.ui.view">
        <field name="name">view.asset.config.form</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="50"/>
        <field name="inherit_id" ref="account_accountant.res_config_settings_view_form" />
        <field name="arch" type="xml">
	        <div id="fiscalyear" position="after">
	            <h2>Day of Car Asset</h2>
                 
                <div class="col-xs-12 col-md-6 o_setting_box" >
                    <div class="o_setting_left_pane">
                        <group>
                            <field name="expiry_before_days_vehicle_registration_expired" />
                            <field name="expiry_before_days_date_of_the_periodic_examination" /> 
                            <field name="expiry_before_days_periodic_maintenance_history" /> 
                            <field name="expiry_before_days_periodic_maintenance_history_month" /> 
                            <field name="expiry_before_days_date_of_the_periodic_examination_month" /> 
                            <field name="expiry_before_days_periodic_maintenance_history_month" />  

                        </group>
                    </div>
                </div>
             </div>
		</field>
	</record>
    
   
  </data>
</odoo>